#include "main.h"

/**
 * _printf - handling specifiers
 * @format: string
 * @...: parameter
 * Return: count of characters
 */
int _printf(const char *format, ...)
{
	va_list print;
	int chara_count = 0;

	va_start(print, format);
	while (*format)
	{
		if (*format == 'd' || *format == 'i')
		{
			int num = va_arg(print, int);
			putchar(num);
			chara_count++;
		}
		format++;
	}
	va_end(print);
	return (chara_count);
}
